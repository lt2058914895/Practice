workspace '/Users/lt/Desktop/Practice/Workspace.xcworkspace'
# workspace的主工程路径，是相对于workspace的路径
project 'ProjectA/ProjectA.xcodeproj'

target 'ProjectA' do
  platform :ios, '9.0'
  pod 'Masonry' 
  pod 'SnapKit'
  pod 'ReactiveCocoa'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'AFNetworking' 
end


target 'ProjectB' do
  platform :ios, '9.0'
# 第二个工程的相对路径，是相对于workspace的路径
  project 'ProjectB/ProjectB.xcodeproj'
  pod 'Masonry' 
  pod 'SnapKit'
  pod 'ReactiveCocoa'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'AFNetworking' 
end


target 'ProjectC' do
  platform :ios, '9.0'
# 第三个工程的相对路径，是相对于workspace的路径
  project 'ProjectC/ProjectC.xcodeproj'
  pod 'Masonry'
  pod 'SnapKit'
  pod 'ReactiveCocoa'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'AFNetworking'
end


